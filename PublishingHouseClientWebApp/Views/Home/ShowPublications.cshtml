@model PublishingHouseClientWebApp.ViewModel.PublicationViewModel

@{
    ViewBag.Title = "ShowPublications";
}
<h2>Публикации</h2>
<table cellpadding="0" cellspacing="0" border="1">
    <tr>
        <th>Название</th>
        <th>УДК</th>
        <th>Номер журнала</th>
        <th>Номер регистрации</th>
        <th>Дата регистрации</th>
        <th>Автор/ы</th>
    </tr>
    @foreach (var publication in Model.Publications)
    {
        <tr>
            <td>@publication.PublicationName</td>
            <td>@publication.UDK</td>
            <td>@publication.Magazine</td>
            <td>@publication.RegistrationNum</td>
            <td>@publication.RegDate</td>
            <td>
                <table>
                    <tr>
                        @foreach (var author in publication.Authors)
                        {
                        <td>@author &nbsp;&nbsp;&nbsp;&nbsp;</td> 
                        }
                    </tr>
                </table>
            </td>
        </tr>
    }
</table>